{
  "name": "@raulanatol/utilsh",
  "version": "1.0.3",
  "author": {
    "name": "Ra√∫l Anatol",
    "email": "hello@raulanatol.com"
  },
  "description": "CLI utility tool with plugin system",
  "main": "dist/index.js",
  "bin": {
    "utilsh": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json}\"",
    "format:check": "prettier --check \"**/*.{ts,json}\"",
    "prepare": "sh scripts/prepare.sh",
    "preinstall": "sh scripts/preinstall.sh",
    "dev": "ts-node src/index.ts",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@types/node": "20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "@vitest/coverage-v8": "3.1.4",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "jsonc-eslint-parser": "2.4.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-sort-json": "4.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vitest": "3.1.4"
  },
  "dependencies": {
    "commander": "12.0.0",
    "glob": "11.0.2",
    "inquirer": "12.6.3"
  },
  "lint-staged": {
    "*.{ts,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
